<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LoadODI</servlet-name>
        <servlet-class>views.LoadODI</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadIPL</servlet-name>
        <servlet-class>views.LoadIPL</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadT20I</servlet-name>
        <servlet-class>views.LoadT20I</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadBBL</servlet-name>
        <servlet-class>views.LoadBBL</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoadODI</servlet-name>
        <url-pattern>/LoadODI</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoadIPL</servlet-name>
        <url-pattern>/LoadIPL</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoadT20I</servlet-name>
        <url-pattern>/LoadT20I</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoadBBL</servlet-name>
        <url-pattern>/LoadBBL</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
