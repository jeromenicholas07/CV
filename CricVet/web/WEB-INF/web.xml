        <servlet-name>LoadPSL</servlet-name>
        <servlet-class>views.LoadPSL</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadCPL</servlet-name>
        <servlet-class>views.LoadCPL</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadTest</servlet-name>
        <servlet-class>views.LoadTest</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadAll</servlet-name>
        <servlet-class>views.LoadAll</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoadODI</servlet-name>
        <url-pattern>/LoadODI</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoadIPL</servlet-name>
