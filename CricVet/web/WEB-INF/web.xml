        <servlet-name>LoadBPL</servlet-name>
        <url-pattern>/LoadBPL</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoadPSL</servlet-name>
        <url-pattern>/LoadPSL</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoadCPL</servlet-name>
        <url-pattern>/LoadCPL</url-pattern>
    </servlet-mapping>
