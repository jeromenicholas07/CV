        <servlet-name>LoadTest</servlet-name>
        <url-pattern>/LoadTest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoadAll</servlet-name>
        <url-pattern>/LoadAll</url-pattern>
    </servlet-mapping>
